{
  "name": "didyoumean2",
  "version": "1.3.0",
  "license": "MIT",
  "main": "src/index.js",
  "engines": {
    "node": ">=4.2"
  },
  "sideEffects": false,
  "scripts": {
    "ignore-sync": "ignore-sync",
    "lint": "eslint .",
    "precommit": "lint-staged && yarn test",
    "test": "cross-env NODE_ENV=test jest"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": ["lcov", "text-summary"]
  },
  "lint-staged": {
    "*.js": ["prettier-eslint --write", "eslint --fix", "git add"],
    "*.json": ["prettier-eslint --write", "git add"]
  },
  "dependencies": {
    "leven": "^2.0.0",
    "lodash.deburr": "^4.1.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.0.0",
    "eslint": "^4.8.0",
    "eslint-config-foray1010": "^6.2.0",
    "husky": "^0.14.0",
    "ignore-sync": "^1.0.0",
    "jest": "^22.0.0",
    "lint-staged": "^7.0.0",
    "prettier-eslint-cli": "^4.4.0",
    "seamless-immutable": "^7.0.0"
  },
  "description":
